<%= render "layouts/errors", :object => @list %>

<%= form_for @list do |f| %>
  <div class="control-group">
    <%= f.label :name %>
    <div class="controls">
      <%= f.text_field :name %>
    </div>
  </div>

  <div class="form-actions">

    <%= f.submit class: "list btn btn-danger" %>

  </div>

  <% end %>
